{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "http://stage.kaedeco.quest" }
          ]
        },
        {
          "source": "stageapi.kaedeco.quest/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "http://stage.kaedeco.quest" }
          ]
        }  
    ]
}
